                <div class="bsProduct_des">
                    <p class="bsProduct_name"> Denim Jacket</p>
                    <p class="bsProduct_price"> <strong>$40.00</strong></p>
                </div>
            </div>
            <div class="bsProduct">
                    <div><img class="bsProduct_img" src="./img/tshirt.png" alt=""></div>
                    <div class="bsProduct_des">
                        <p class="bsProduct_name"> Ford T-shirt</p>
                        <p class="bsProduct_price"> <strong>$20.00</strong></p>
                    </div>
            </div>
            <div class="bsProduct">
                <div><img class="bsProduct_img" src="./img/shoes.png" alt=""></div>
                <div class="bsProduct_des">
                    <p class="bsProduct_name"> VNG Shoes</p>
                    <p class="bsProduct_price"> <strong>$70.00</strong></p>
                </div>
            </div>
            <div class="bsProduct">
                <div><img class="bsProduct_img" src="./img/sweater.png" alt=""></div>
                <div class="bsProduct_des">
                    <p class="bsProduct_name"> Striped Sweater</p>
                    <p class="bsProduct_price"> <strong>$50.00</strong></p>
                </div>
            </div>
        </div>
